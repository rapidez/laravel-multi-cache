{
    "name": "rapidez/laravel-multi-cache",
    "type": "package",
    "description": "Combine multiple caches in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Indy Koning"
        },
        {
            "name": "Anthony Kuske",
            "homepage": "https://anthonykuske.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0 || ^11.1"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0|^8.0",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\LaravelMultiCache\\MultiStoreServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\LaravelMultiCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rapidez\\LaravelMultiCacheTests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
